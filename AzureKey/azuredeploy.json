{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vaults_KeyVaultas_name": {
      "defaultValue": "KeyVaultas",
      "type": "String"
    },
    "secretName": {
      "defaultValue": "vmadmi",
      "type": "string"
    },
    "secretValue": {
      "type": "securestring"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.KeyVault/vaults",
      "apiVersion": "2024-04-01-preview",
      "name": "[parameters('vaults_KeyVaultas_name')]",
      "location": "canadacentral",
      "properties": {
        "sku": {
          "family": "A",
          "name": "Standard"
        },
        "tenantId": "185bfdad-60f6-4944-83f3-f49a95deee1a",
        "accessPolicies": [],
        "enabledForDeployment": true,
        "enabledForDiskEncryption": false,
        "enabledForTemplateDeployment": true,
        "enableSoftDelete": true,
        "softDeleteRetentionInDays": 7,
        "enableRbacAuthorization": true,
        "vaultUri": "https://keyvaultas.vault.azure.net/",
        "provisioningState": "Succeeded",
        "publicNetworkAccess": "Enabled"
      }
    },
    {
      "type": "Microsoft.KeyVault/vaults/secrets",
      "apiVersion": "2024-04-01-preview",
      "name": "[concat(parameters('vaults_KeyVaultas_name'), '/vmadmi')]",
      "location": "canadacentral",
      "dependsOn": [
        "[resourceId('Microsoft.KeyVault/vaults', parameters('vaults_KeyVaultas_name'))]"
      ],
      "properties": {
        "attributes": {
          "enabled": true
        }
      }
    }
  ]
}